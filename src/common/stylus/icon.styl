    url('../fonts/sell-icon.ttf?nbq4u4') format('truetype'),
    url('../fonts/sell-icon.woff?nbq4u4') format('woff'),
    url('../fonts/sell-icon.svg?nbq4u4#sell-icon') format('svg')
